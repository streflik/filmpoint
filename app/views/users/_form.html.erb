<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email%>
    <%= f.input :password%>
    <%= f.input :password_confirmation%>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
